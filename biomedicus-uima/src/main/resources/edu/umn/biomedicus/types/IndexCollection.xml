<fsIndexCollection xmlns="http://uima.apache.org/resourceSpecifier">
  <name>BioMedICUS Standard Indexes</name>
  <description>The fs indexes for the BioMedICUS System</description>
  <version>${project.version}</version>
  <vendor>${project.organization.name}</vendor>

  <fsIndexes>
    <fsIndexDescription>
      <label>artifactID</label>
      <typeName>ArtifactID</typeName>
      <keys>
        <fsIndexKey>
          <featureName>artifactID</featureName>
          <comparator>standard</comparator>
        </fsIndexKey>
      </keys>
    </fsIndexDescription>
    <fsIndexDescription>
      <label>metadata</label>
      <typeName>ArtifactMetadata</typeName>
      <keys>
        <fsIndexKey>
          <featureName>key</featureName>
          <comparator>standard</comparator>
        </fsIndexKey>
      </keys>
    </fsIndexDescription>
  </fsIndexes>
</fsIndexCollection>